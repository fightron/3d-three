{
  "name": "@fightron/skeleton-three",
  "version": "0.0.0",
  "description": "Three wrapper for @fightron/skeleton-js",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test c8 mocha",
    "test:ci": "NODE_ENV=test mocha",
    "lint": "standardx --fix --verbose",
    "release": "release-it --config ./node_modules/@dimensionalpocket/release/config/default.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fightron/skeleton-skeleton.git"
  },
  "author": "fightron.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fightron/skeleton-skeleton/issues"
  },
  "homepage": "https://github.com/fightron/skeleton-skeleton#readme",
  "dependencies": {
    "three": "0.138.3"
  },
  "peerDependencies": {
    "@fightron/3d": "github:fightron/3d-js#semver:0.x"
  },
  "devDependencies": {
    "@dimensionalpocket/development": "github:dimensionalpocket/development-js#0.5.2",
    "@dimensionalpocket/release": "github:dimensionalpocket/release-js#0.2.4",
    "@fightron/3d": "github:fightron/3d-js#main",
    "@types/three": "0.138.0",
    "canvas": "2.9.0",
    "jsdom": "19.0.0"
  },
  "babel": {
    "extends": "./node_modules/@dimensionalpocket/development/babel"
  },
  "eslintConfig": {
    "extends": "./node_modules/@dimensionalpocket/development/eslint"
  },
  "mocha": {
    "recursive": true,
    "reporter": "dot",
    "colors": true
  },
  "c8": {
    "extends": "./node_modules/@dimensionalpocket/development/c8/default.json",
    "include": [
      "index.js",
      "src"
    ]
  },
  "engines": {
    "node": ">= 14.17.1"
  }
}
